{
  "name": "platzi-webpack",
  "version": "1.0.0",
  "description": "Curso de Webpack de Platzi",
  "main": "index.js",
  "scripts": {
    "build": "webpack index.js --mode development",
    "build:local": "webpack --mode development",
    "build:external": "webpack --mode development --config ./external/webpack.config.js",
    "build:css": "webpack --mode development --config ./css-style-loaders/webpack.config.js",
    "build:extract:css": "webpack --mode development --config ./plugin-extract-text/webpack.config.js",
    "build:multi": "webpack --mode development --config ./multiples-entry-points/webpack.config.js",
    "build:dev": "webpack-dev-server --mode development --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --mode development --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --mode development --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --mode development --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --mode development --config ./file-loader-video/webpack.config.js",
    "build:json": "webpack --mode development --config ./json-loader/webpack.config.js",
    "build:react": "webpack --mode development --config ./react/webpack.config.js",
    "build:sass": "webpack --mode development --config ./sass-loader/webpack.config.js",
    "build:stylus": "webpack --mode development --config ./stylus-loader/webpack.config.js",
    "build:less": "webpack --mode development --config ./less-loader/webpack.config.js",
    "build:postcss": "webpack --mode development --config ./postcss-loader/webpack.config.js",
    "build:prevent": "webpack --mode development --config ./prevent-duplication/webpack.config.js",
    "build:vendor": "webpack --mode development --config ./vendor/webpack.config.js",
    "build:dll": "webpack --mode development --config ./dll/webpack.dll.config.js",
    "build:dll:src": "webpack --mode development --config ./dll/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hecto932/platzi-webpack.git"
  },
  "author": "Hector Flores <hecto932@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hecto932/platzi-webpack/issues"
  },
  "homepage": "https://github.com/hecto932/platzi-webpack#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "nib": "^1.1.2",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.22",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "rupture": "^0.7.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
